n'}}> \n        <ListItem style={{position: 'relative', color: 'teal', padding: 12, fontSize: 20, justifyContent: 'flex-start'}}>\n           <img src={'/assets/teal.png'} alt=\"Workhub\" style={{height: 33, marginRight: minimized ? 0 : 8, marginLeft: minimized ? 0: 0}} /> \n\n           {!minimized && <Typography>Workhub</Typography>}\n\n           <IconButton size=\"small\" style={{backgroundColor: 'green', zIndex: 9, position: 'absolute', right: -12, bottom: -12}} onClick={() => setMinimized(!minimized)}>\n              {minimized ? <ChevronRight style={{color: 'rgb(222,222,222)'}}/> : <ChevronLeft style={{color: 'rgb(222,222,222)'}} />}  \n           </IconButton>\n          \n        </ListItem>\n        <Divider />\n        {menu.map((x, ix) => (\n            <ListItem \n            className={menu.map((x) => x.path).indexOf(window.location.pathname.split(props.match.url)[1]) == ix ? 'selected menu-item': 'menu-item'}\n            onClick={() => props.history.push(`${props.match.url}${x.path}`)}\n            button >\n              {x.icon} \n              {!minimized && x.label}\n            </ListItem>\n        ))}\n        </List>\n        <Divider />\n        <ListItem style={{justifyContent: minimized ? 'center' : \"initial\"}} button onClick={() => props.history.push(`${props.match.url}/settings`)}>\n            <Settings style={{marginRight: minimized ? 0 : 12}} />\n            {!minimized && <Typography>Settings</Typography>}\n        </ListItem>\n        </Paper>\n    )\n}\n\nexport default withRouter(Sidebar)","/home/workhub/work/hub/packages/web-ui/src/views/Dashboard/types/calendar.tsx",["189"],"/home/workhub/work/hub/packages/web-ui/src/views/Dashboard/types/project.tsx",[],"/home/workhub/work/hub/packages/web-ui/src/views/Dashboard/types/file.tsx",["190"],"import { WorkhubClient } from \"@workerhive/client\";\nimport { FileBrowser, Header } from \"@workerhive/react-ui\";\nimport React from \"react\";\n\nexport const FILE_VIEW = {\n        path: '/dashboard/files',\n        label: \"Files\",\n        data: {\n            files: {\n                type: '[File]'\n            }\n        },\n        layout: (sizes: any, rowHeight: number) => [\n            {\n                i: 'header',\n                x: 0,\n                y: 0,\n                w: 12, \n                h: 1,\n                component: (data: any) => (<Header title=\"Files\" />)\n            },\n            {\n                i: 'data',\n                x: 0,\n                y: 1,\n                w: 12,\n                h: (sizes.height / rowHeight) -2,\n                component: (data: any, params: any, types: any, client: any) => (\n                    <FileBrowser files={data.files} onFileUpload={({files}: any) => {\n                        client!.actions.addFile(files[0])\n                    }} />\n                )\n            }\n        ]\n    }","/home/workhub/work/hub/packages/web-ui/src/views/Dashboard/types/equipment.tsx",[],"/home/workhub/work/hub/packages/web-ui/src/views/Dashboard/types/knowledgebase.tsx",[],"/home/workhub/work/hub/packages/web-ui/src/views/Dashboard/types/contacts.tsx",[],"/home/workhub/work/hub/packages/web-ui/src/views/Dashboard/types/home.tsx",[],"/home/workhub/work/hub/packages/web-ui/src/views/Dashboard/types/admin.tsx",[],"/home/workhub/work/hub/packages/web-ui/src/views/Dashboard/types/team.tsx",["191","192"],"import { Divider, Fab, ListItem, Typography } from \"@material-ui/core\";\nimport { Add, Delete, Edit } from \"@material-ui/icons\";\nimport { Header, MoreMenu, MutableDialog, SearchTable } from \"@workerhive/react-ui\";\nimport React from \"react\";\n\nexport const TEAM_VIEW = {\n        path: '/dashboard/team',\n        label: \"Team\",\n        data: {\n            team: {\n                type: \"[TeamMember]\"\n            }\n        },\n        layout: (sizes : any, rowHeight: number) => [\n            {\n                i: 'header',\n                x: 0,\n                y: 0,\n    