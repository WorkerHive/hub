{
  "npmClient": "yarn",
  "packages": [
    "packages/*"
  ],
  "command": {
    "publish": {
      "ignoreChanges": [
        "server/**",
        "web-ui/**",
        "*.md"
      ],
      "message": "chore(release): Publish"
    },
    "changed": {
      "ignoreChanges": [
        "server/*",
        "web-ui/*",
        "*.md"
      ]
    }
  },
  "version": "2.1.1-alpha.0"
}
